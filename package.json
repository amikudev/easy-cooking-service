{
  "name": "east-cook-services",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "deploy:heroku": "git push origin heroku",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cloudrun:deploy": "gcloud run deploy --image asia.gcr.io/easy-cooking-281412/easy-cooking-service:first --platform managed",
    "docker:build": "docker build -t easy-cook-service .",
    "docker:tag": "docker tag easy-cook-service  asia.gcr.io/easy-cooking-281412/easy-cooking-service:first",
    "docker:push": "docker push asia.gcr.io/easy-cooking-281412/easy-cooking-service:first",
    "docker:publish": "npm run docker:build && npm run docker:tag && npm run docker:push",
    "prettier": "npx prettier --write ."
  },
  "dependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.6.13",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/typeorm": "^7.1.5",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "firebase-admin": "^9.5.0",
    "mongoose": "^5.11.18",
    "nestjs-config": "^1.4.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.8",
    "@nestjs/testing": "^7.6.13",
    "@types/express": "^4.17.11",
    "@types/jest": "25.2.3",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^13.13.45",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.1.3",
    "jest": "26.0.1",
    "lint-staged": "^10.5.4",
    "prettier": "1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "14.15.4"
  }
}
